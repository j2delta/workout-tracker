<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="WorkoutTracker.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:WorkoutTracker"
    Shell.FlyoutBehavior="Disabled"
    Title="WorkoutTracker">

    <!--<ShellContent
        Title="Workouts"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />-->

    <ShellContent
    Title="Workouts"
    ContentTemplate="{DataTemplate local:WorkoutsPageView}"
    Route="WorkoutsPageView" />

    <ShellContent
    Title="Exercises"
    ContentTemplate="{DataTemplate local:ExercisesPageView}"
    Route="ExercisesPageView" />

    <ShellContent
    Title="New Workout"
    ContentTemplate="{DataTemplate local:NewWorkoutPageView}"
    Route="NewWorkoutPageView" />


</Shell>
